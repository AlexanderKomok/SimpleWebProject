@model WebAppTry3.Controllers.PlayerController.FullTrackList

<div class="container">
    <div class="col-md-5">
        <b>Now Playing</b>
        <div class="list-item" id="playing">
            @*Output Play*@
            @foreach (var item in Model.ListPlay)
            {


                <div class="item">
                    <div id="playing1" @*class="name" *@><span>@item.TrackName</span></div>
                </div>

            }

        </div>
    </div>
    <div class="col-md-5">
        <b>List to play</b>
        <div class="list-item" id="list-to-play">
            @*<div class="item">
            *@
            @*output PlayList*@
            @foreach (var item in Model.ListToPlay)
            {
                <div class="item">
                    <div id="list-to-play1"><span>@item.TrackName</span></div>
                </div>
            }
            @*
                </div>*@
        </div>
    </div>
    <div class="col-md-5">
        <b>History</b>
        <div class="list-item" id="history">
            @*<div class="item">
            *@
            @*Output history*@
            @foreach (var item in Model.ListHistory)
            {
                <div class="item">
                    <div id="history1"><span>@item.TrackName</span></div>
                </div>
            }
            @*
                </div>*@
        </div>
    </div>
</div>

