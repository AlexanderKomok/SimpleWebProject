@model WebAppTry3.DBEntities.Track

    <form>
        <div class="form-group">
            <label asp-for="User" class="control-label"></label>
            @*select asp-for="UserId" create list item for UserId*@
            <select asp-for="UserId" class="form-control" asp-items="ViewBag.UserId"></select>
            <span asp-validation-for="User.Id" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="AlbumName" class="control-label"></label>
            <select asp-for="AlbumName" class="form-control" asp-items="ViewBag.AlbumName"></select>
        </div>
        <div class="form-group">
            <label asp-for="TrackUrl" class="control-label"></label>
            <input asp-for="TrackUrl" class="form-control" />
            <span asp-validation-for="TrackUrl" class="text-danger"></span>
        </div>

    </form>
    <form asp-action="Index">

    </form>